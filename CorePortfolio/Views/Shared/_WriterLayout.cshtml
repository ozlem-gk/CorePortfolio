
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">


@await Html.PartialAsync("/Areas/Writer/Views/Default/HeadPartial.cshtml")

<body>
    <div class="container-scroller">
        <!-- partial:../../partials/_navbar.html -->
        @await Html.PartialAsync("/Areas/Writer/Views/Default/_NavbarPartial.cshtml")
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
   
            <!-- partial:../../partials/_sidebar.html -->
            @await Html.PartialAsync("/Areas/Writer/Views/Default/_SideBarPartial.cshtml")
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="row">    
                        @RenderBody()
                    </div>
                </div>
                @await Html.PartialAsync("/Areas/Writer/Views/Default/_FooterPartial.cshtml")
            </div>
        </div>
    </div>
    @await Html.PartialAsync("/Areas/Writer/Views/Default/_ScriptsPartial.cshtml")
</body>

</html>
